<div id="nav-links" class="list-group">
  <% request_groups.each do |request_group| %>
    <div class="list-group-item">
      <h4 class="list-group-item-heading"><%= request_group.title %></h4>
      <ul class="list-unstyled">
        <% request_group.request_classes.each do |request_class| %>
          <%= nav_link request_class.label, path_to_new_request(request_class) %>
        <% end %>
      </ul>
    </div>
  <% end %>
  <% if current_user.can? :read, Support::Requests::Anonymous::ProblemReport %>
    <div class="list-group-item">
      <h4 class="list-group-item-heading">Anonymous feedback</h4>
      <ul class="list-unstyled">
        <%= feedex_nav_link %>
      </ul>
    </div>
  <% end %>
</div>
