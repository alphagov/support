require 'zendesk/ticket/<%= zendesk_ticket_model_filename %>'
require 'support/requests/<%= model_filename %>'

class <%= controller_class_name %> < RequestsController
  include Support::Requests

  protected
  def new_request
    <%= model_class_name %>.new
  end

  def zendesk_ticket_class
    Zendesk::Ticket::<%= zendesk_ticket_model_class_name %>
  end

  def parse_request_from_params
    <%= model_class_name %>.new(params[:support_requests_<%= model_filename %>])
  end
end
